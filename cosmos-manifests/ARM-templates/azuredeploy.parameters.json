{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "accountName": {
      "value": "jmulla-cosmoslab"
    },
    "primaryRegion": {
      "value": "West US 2"
    },
    "databaseName": {
      "value": "todos"
    },
    "containerName": {
      "value": "tasks"
    },
    "throughput": {
      "value": 400
    },
    "tasksContainer": {
      "value": {
        "name": "tasks",
        "partitionKey": {
          "paths": [
            "/partitionKey"
          ],
          "kind": "Hash"
        },
        "indexingPolicy": {
          "indexingMode": "consistent",
          "includedPaths": [
            {
              "path": "/*"
            }
          ]
        },
        "defaultTtl": 1209600, 
        "uniqueKeyPolicy": {
						"uniqueKeys": [
							{
								"paths": []
							}
						]
          },
        "options": {
          "throughput": 400
        }
      }
    }
  }
}
